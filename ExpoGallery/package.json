    "expo-linking": "~7.0.5",
    "expo-location": "~18.0.8",
    "expo-notifications": "~0.29.14",
    "expo-router": "~4.0.19",
    "expo-secure-store": "~14.0.1",
    "expo-sensors": "~14.0.2",
    "expo-speech": "~13.0.1",
    "expo-speech-recognition": "^1.1.1",
    "expo-splash-screen": "~0.29.22",
    "expo-status-bar": "~2.0.1",

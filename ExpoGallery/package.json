{
  "name": "expogallery",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "dotenv -e .env.local -- expo start",
    "android": "dotenv -e .env.local -- expo run:android",
    "ios": "dotenv -e .env.local -- expo run:ios",
    "web": "dotenv -e .env.local -- expo start --web --clear",
    "web:clean": "rm -rf node_modules/.cache && dotenv -e .env.local -- expo start --web --clear",
    "serve-build": "npx serve dist",
    "test": "dotenv -e .env.local -- jest --ci",
    "watchTests": "dotenv -e .env.local -- jest --watchAll",
    "lint": "expo lint",
    "create-version-json": "dotenv -e .env.local -- node ./scripts/create-version-json.js",
    "predeploy:gh-pages": "npm run create-version-json && WEB=true EXPO_PUBLIC_BASE_URL=/ExpoGallery dotenv -e .env.local -- expo export -p web",
    "deploy:gh-pages": "gh-pages --nojekyll -d dist",
    "predeploy:root": "npm run create-version-json && WEB=true EXPO_PUBLIC_BASE_URL=/ dotenv -e .env.local -- expo export -p web",
    "deploy:root": "echo 'Ready to deploy dist folder to your root domain server'",
    "predeploy": "npm run predeploy:gh-pages",
    "deploy": "npm run deploy:gh-pages",
    "check-env": "node -e \"require('./scripts/check-env.js')\"",
    "check-web-env": "node -e \"require('./scripts/check-web-env.js')\""
  },
  "dependencies": {
    "@11labs/react": "^0.0.8",
    "@expo/metro-runtime": "~4.0.1",
    "@expo/vector-icons": "^14.0.2",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/netinfo": "11.4.1",
    "@react-native-picker/picker": "^2.9.0",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@shopify/flash-list": "1.7.3",
    "@teovilla/react-native-web-maps": "0.9.5",
    "dayjs": "^1.11.10",
    "eas-cli": "^16.0.0",
    "eventemitter3": "^5.0.1",
    "expo": "~52.0.38",
    "expo-asset": "~11.0.4",
    "expo-auth-session": "~6.0.3",
    "expo-battery": "~9.0.2",
    "expo-blur": "~14.0.3",
    "expo-brightness": "~13.0.3",
    "expo-cellular": "~7.0.2",
    "expo-clipboard": "~7.0.1",
    "expo-constants": "~17.0.7",
    "expo-crypto": "~14.0.2",
    "expo-dev-client": "~5.0.14",
    "expo-device": "~7.0.2",
